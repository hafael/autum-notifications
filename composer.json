{
    "name": "hafael/autum-notifications",
    "description": "Autum Notifications Central",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": ["autum", "php", "laravel", "notifications", "vue"],
    "require": {
        "php" : ">=5.6",
        "laravel/framework": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.5"
    },
    "authors": [
        {
            "name": "Rafael Villa Verde",
            "email": "rafael@autum.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "Autum\\Notifications\\": "src/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Autum\\Notifications\\AutumNotificationServiceProvider"
            ]
        }
    }
}
